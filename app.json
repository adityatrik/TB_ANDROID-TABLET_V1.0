{
  "expo": {
    "name": "TB CR 2",
    "slug": "TB-CR-2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon-1.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-1.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "packagerOpts": {
      "sourceExts": [
        "js",
        "ts",
        "tsx",
        "jsx"
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "extra": {
      "eas": {
        "projectId": "19a139f2-ce53-48eb-b425-c2754716094a"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-1.png",
        "backgroundColor": "#000000"
      },
      "package": "com.adityatrik2.tanblackandroid"
    }
  }
}
